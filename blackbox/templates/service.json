{
  "apiVersion": "v1",
  "kind": "List",
  "items": [
    {
      "kind": "Service",
      "apiVersion": "v1",
      "metadata": {
        "name": "",
        "labels": {
          "app": ""
        }
      },
      "spec": {
        "ports": [
          {
            "name": "3001-3001",
            "protocol": "TCP",
            "port": 3001,
            "targetPort": 3001,
            "nodePort": 31000
          }
        ],
        "selector": {
          "app": ""
        },
        "type": "NodePort",
        "ipFamilies": ["IPv4"]
      }
    },
    {
      "kind": "Deployment",
      "apiVersion": "apps/v1",
      "metadata": {
        "name": "",
        "labels": {
          "app": ""
        }
      },
      "spec": {
        "replicas": 1,
        "selector": {
          "matchLabels": {
            "app": ""
          }
        },
        "template": {
          "metadata": {
            "labels": {
              "app": ""
            }
          },
          "spec": {
            "terminationGracePeriodSeconds": 30,
            "containers": [
              {
                "name": "",
                "image": "",
                "ports": [
                  {
                    "containerPort": 3001
                  }
                ],
                "lifecycle": {
                  "preStop": {
                    "exec": { "command": ["sleep", "20"] }
                  }
                },
                "startupProbe": {
                  "initialDelaySeconds": 5,
                  "periodSeconds": 2,
                  "timeoutSeconds": 2,
                  "successThreshold": 3,
                  "failureThreshold": 10,
                  "httpGet": {
                    "scheme": "HTTP",
                    "path": "/",
                    "port": 3001
                  }
                },
                "readinessProbe": {
                  "initialDelaySeconds": 5,
                  "periodSeconds": 2,
                  "timeoutSeconds": 2,
                  "successThreshold": 3,
                  "failureThreshold": 5,
                  "httpGet": {
                    "scheme": "HTTP",
                    "path": "/",
                    "port": 3001
                  }
                }
              }
            ]
          }
        }
      }
    }
  ]
}
